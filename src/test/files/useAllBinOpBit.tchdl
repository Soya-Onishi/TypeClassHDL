package test

module Top
impl Top {
  input def ops(x: Bit[8], y: Bit[8], z: Int) -> Unit {
    val a = x + y
    val b = x - y
    val c = x * y
    val d = x / y
    val e = x == y
    val f = x != y
    val g = x <  y
    val h = x <= y
    val i = x >  y
    val j = x >= y
    val k = x >> 2
    val l = x << 3
    val m = x >> z
    val n = x << z
    val o = x >>> y
    val p = x <<< y
    val q = x | y
    val r = x & y
    val s = x ^ y
  }
}
